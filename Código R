 Base_de_Datos_2018_2020_II_ %>% filter(AÑO==2020) %>%  group_by(CURSO) %>% summarise(promedio=mean(PROMEDIO),minimo=min(PROMEDIO), mediana= median(PROMEDIO), moda=mlv(PROMEDIO), maximo=max(PROMEDIO))


